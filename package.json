{
  "name": "oauth-auth-examples",
  "version": "1.0.0",
  "description": "Comprehensive OAuth authentication examples with Google, Microsoft, and GitHub",
  "main": "server/index.js",
  "scripts": {
    "start": "node server/index.js",
    "dev": "nodemon server/index.js",
    "client": "live-server client/public --port=3001",
    "server": "nodemon server/index.js",
    "dev:full": "concurrently \"npm run server\" \"npm run client\"",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "migrate": "node scripts/migrate.js",
    "seed": "node scripts/seed.js",
    "lint": "eslint . --ext .js",
    "format": "prettier --write \"**/*.{js,json,css,html}\"",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f"
  },
  "keywords": [
    "oauth",
    "authentication",
    "google",
    "microsoft",
    "github",
    "jwt",
    "express",
    "nodejs"
  ],
  "author": "Dennis",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "express-session": "^1.17.3",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "bcrypt": "^5.1.1",
    "jsonwebtoken": "^9.0.2",
    "passport": "^0.7.0",
    "passport-google-oauth20": "^2.0.0",
    "passport-microsoft": "^1.0.0",
    "passport-github2": "^0.1.12",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "mongoose": "^8.0.3",
    "pg": "^8.11.3",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.35.2",
    "redis": "^4.6.12",
    "connect-redis": "^7.1.0",
    "uuid": "^9.0.1",
    "validator": "^13.11.0",
    "express-validator": "^7.0.1",
    "winston": "^3.11.0",
    "morgan": "^1.10.0",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "csurf": "^1.11.0",
    "speakeasy": "^2.0.0",
    "qrcode": "^1.5.3",
    "nodemailer": "^6.9.7",
    "handlebars": "^4.7.8",
    "multer": "^1.4.5-lts.1",
    "sharp": "^0.33.1",
    "axios": "^1.6.3",
    "config": "^3.3.9"
  },
  "devDependencies": {
    "nodemon": "^3.0.2",
    "jest": "^29.7.0",
    "supertest": "^6.3.3",
    "eslint": "^8.56.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.29.1",
    "prettier": "^3.1.1",
    "husky": "^8.0.3",
    "lint-staged": "^15.2.0",
    "concurrently": "^8.2.2",
    "live-server": "^1.2.2",
    "@types/node": "^20.10.6",
    "@types/express": "^4.17.21"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dnoice/oauth-auth-examples.git"
  },
  "bugs": {
    "url": "https://github.com/dnoice/oauth-auth-examples/issues"
  },
  "homepage": "https://github.com/dnoice/oauth-auth-examples#readme"
}
